# Supabase 配置
# 警告：这些是敏感凭据，请不要提交到版本控制系统
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# 其他配置
VITE_APP_NAME=OneTab Plus
VITE_APP_VERSION=1.5.12

# 安全配置
# 设置为 true 时启用额外的安全检查
VITE_ENABLE_SECURITY_CHECKS=true

# 开发环境配置
# 设置为 true 时启用开发专用功能（如模拟登录）
VITE_DEV_MODE=false